    "react-dom": "^17.x || ^18.x || ^19.x"
  },
  "author": "Quasiris Team",
  "type": "commonjs",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1"
  }
