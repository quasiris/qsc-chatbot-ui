{
  "name": "@quasiris/qsc-chatbot-ui",
  "version": "1.3.7",
  "description": "A plugin that injects a Chatbot UI with WS/REST support",
  "main": "lib/index.js",
  "unpkg": "qsc-chatbot.js",
  "module": "lib/index.esm.js",
  "scripts": {
    "build": "babel src --out-dir lib --extensions \".js,.jsx,.ts,.tsx\" --copy-files && rollup -c",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "dist/qsc-chatbot.js"
  ],
  "keywords": [
    "Quasiris",
    "chatbot-ui",
    "chatbot-ui-react",
    "chatbot-ui-webcomponent",
    "chatbot-ui-plugin-react",
    "chatbot-ui-plugin-webcomponent",
    "chatbot-ui-plugin-docusaurus",
    "docusaurus",
    "QSC",
    "webComponent",
    "chatbot",
    "react",
    "plugin"
  ],
  "peerDependencies": {
    "@docusaurus/core": "^3.x",
    "react": "^17.x || ^18.x || ^19.x",
    "react-dom": "^17.x || ^18.x || ^19.x"
  },
  "author": "Quasiris Team",
  "type": "commonjs",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.45.1"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "marked": "^16.1.1"
  }
}
